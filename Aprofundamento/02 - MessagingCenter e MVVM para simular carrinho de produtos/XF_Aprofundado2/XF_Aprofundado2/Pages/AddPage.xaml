             x:Class="XF_Aprofundado2.Pages.AddPage">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>

        <Button Text="Adicionar Elemento 1" Clicked="ButtonAddElemento1_OnClicked" Grid.Row="0" />
        <Button Text="Adicionar Elemento 2" Clicked="ButtonAddElemento2_OnClicked" Grid.Row="1" />
        <Button Text="Adicionar Elemento 3" Clicked="ButtonAddElemento3_OnClicked" Grid.Row="2" />
