<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF_EventosPages.MainPage"
             Padding="10,10,10,10">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Button Text="Abrir nova página" Grid.Row="0" Clicked="Button_OnClicked" />
            <Editor IsEnabled="False" x:Name="Editor" Grid.Row="1" />
        </Grid>
    </ContentPage.Content>
</ContentPage>