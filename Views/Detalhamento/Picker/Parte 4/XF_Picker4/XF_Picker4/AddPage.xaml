<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF_Picker4.AddPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <Entry x:Name="EntryVisual" HorizontalOptions="FillAndExpand"/>
            <Entry x:Name="EntryReal" HorizontalOptions="FillAndExpand"/>
            <Button Text="Cancelar" Clicked="ButtonCancelar_OnClicked" />
            <Button Text="Confirmar" Clicked="ButtonConfirmar_OnClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>